[color]
    ui = auto
[alias]
    st = status
    co = checkout
    ci = commit
    br = branch
    gr = log --graph --date=short --format=\"%C(yellow)%h%C(reset) %C(magenta)[%ad]%C(reset)%C(auto)%d%C(reset) %s %C(cyan)@%an%C(reset)\"
[init]
    templatedir = ~/.git_template
[pager]
    grep = false
[alias]
    delete-merged-branches = !git branch --merged | grep -v \\* | xargs -I % git branch -d %
